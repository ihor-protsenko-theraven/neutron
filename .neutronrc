{
  "baseDir": "src",
  "targetDir": "dist",
  "statics": {
    "fonts": ["eot", "ttf", "woff", "woff2"],
    "img": ["png", "svg"]
  },
  "dependencies": {
    "babel": {
      "extensions": ["js", "jsx"]
    },
    "jade": {
      "extensions": ["jade"]
    },
    "sass": {
      "extensions": ["scss", "css"],
      "includePaths": ["src/scss_include"]
    }
  },
  "cleanIgnore": [],
  "packager": {
    "dir": "dist",
    "out": "packages",
    "name": "neutron",
    "platform": "all",
    "arch": "all",
    "version": "0.30.4",
    "overwrite": true
  }
}
